{
    "name": "nom-de-guerre",
    "version": "1.0.0",
    "main": "electron/index.js",
    "repository": "https://github.com/Krosantos/nom-de-guerre",
    "author": "Tymko Tecca <tymko.tecca@gmail.com>",
    "license": "UNLICENSED",
    "scripts": {
        "build": "npx webpack",
        "compile-mac": "yarn build && node electron/prebuild && electron-packager ./staging \"Nom de Guerre\" --asar --icon=./icon --packageManager=yarn --appCategoryType=public.app-category.utilities --out=compiled/ --platform=darwin --no-tmpdir --overwrite",
        "compile-windows": "yarn build && node electron/prebuild && electron-packager ./staging \"Nom de Guerre\" --asar --icon=./icon --packageManager=yarn --out=compiled/ --platform=win32 --no-tmpdir --overwrite",
        "dev": "webpack-dev-server --mode development",
        "docker": "docker build -t ndg . && docker run -p 4000:4000 ndg",
        "lint": "eslint .",
        "serve": "nodemon src/server/index.js --exec \"node -r dotenv/config -r babel-register\"",
        "start": "concurrently --kill-others \"yarn dev\" \"yarn window\"",
        "window": "cross-env FROM_LOCAL=true electron ."
    },
    "dependencies": {
        "axios": "^0.18.0",
        "babel-loader": "7",
        "dotenv": "^6.0.0",
        "dotenv-webpack": "^1.5.7",
        "express": "^4.16.3",
        "lodash": "^4.17.10",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-redux": "^5.0.7",
        "recompose": "^0.30.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "styled-components": "^3.3.0",
        "styled-system": "^2.2.5"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "concurrently": "^3.5.1",
        "cross-env": "^5.2.0",
        "css-loader": "^0.28.11",
        "electron": "^2.0.2",
        "electron-devtools-installer": "^2.2.4",
        "electron-packager": "^12.1.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.8.2",
        "file-loader": "^1.1.11",
        "ncp": "^2.0.0",
        "nodemon": "^1.18.4",
        "style-loader": "^0.21.0",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9"
    }
}

{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"electron": "4.0.1"
				}
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-object-rest-spread",
		"@babel/plugin-transform-runtime",
		[
			"module-resolver",
			{
				"root": [
					"./src"
				],
				"alias": {
					"@api": "./src/client/modules/api",
					"@components": "./src/client/components",
					"@pairs": "./src/client/modules/pairs",
					"@selectors": "./src/client/modules/selectors",
					"@styled": "./src/client/styled"
				},
				"cwd": "packagejson"
			}
		]
	]
}